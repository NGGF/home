<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="GodGenerals" namespace="/GodGenerals" extends="default">
        <action name="GodGeneralsAdd" class="tw.tpe.com.nggf.godGenerals.action.GodGeneralsAction">
            <result type="tiles" name="success">godGeneralsAdd</result>
        </action>
        <action name="GodGeneralsQuery" class="tw.tpe.com.nggf.godGenerals.action.GodGeneralsQueryAction">
            <result type="tiles" name="godGeneralsQuery">godGeneralsQuery</result>
            <result type="tiles" name="godGeneralsQueryList">godGeneralsQueryList</result>
            <result type="tiles" name="godGeneralsQueryView">godGeneralsQueryView</result>
        </action>
        <action name="GodGeneralsEdit" class="tw.tpe.com.nggf.godGenerals.action.GodGeneralsEditAction">
            <result type="tiles" name="godGeneralsEdit">godGeneralsEdit</result>
            <result type="tiles" name="godGeneralsQuery">godGeneralsQuery</result>
        </action>
    </package>
</struts>
